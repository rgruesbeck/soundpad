{
  "metadata": {
    "title": "HTML5 Canvas Game Scaffold",
    "description": "A scaffold for creating games with HTML5 Canvas on Koji. Easily customizable at gokoji.com.",
    "image": "https://images.koji-cdn.com/template-assets/727131f6-5eb2-4965-a7ae-fab193c57ff1_1556937543249.png",
    "icon": "https://images.koji-cdn.com/template-assets/727131f6-5eb2-4965-a7ae-fab193c57ff1_1556937543249.png",
    "icon192": "https://images.koji-cdn.com/template-assets/727131f6-5eb2-4965-a7ae-fab193c57ff1_1556937543249.png?&w=192&h=192",
    "icon512": "https://images.koji-cdn.com/template-assets/727131f6-5eb2-4965-a7ae-fab193c57ff1_1556937543249.png?&w=512&h=512",
    "gaCode": ""
  },
  "@@editor": [
    {
      "key": "metadata",
      "name": "Metadata",
      "icon": "âš™",
      "source": "metadata.json",
      "fields": [
        {
          "key": "title",
          "name": "Name for your App",
          "description": "Will show up as title on Google and Facebook"
        },
        {
          "key": "description",
          "name": "Description",
          "description": "Description of your app for Google and Facebook",
          "type": "textarea"
        },
        {
          "key": "gaCode",
          "name": "GA Tracking ID",
          "description": "Tracking ID for Google Analytics",
          "type": "text",
          "typeOptions": {
            "monospace": true
          }
        },
        {
          "key": "image",
          "name": "Share Image",
          "type": "image",
          "description": "Embedded Image to load when your App is shared"
        },
        {
          "key": "icon",
          "name": "Icon 32 x 32",
          "type": "image",
          "description": "Small Icon for your App"
        },
        {
          "key": "icon192",
          "name": "Icon 192 x 192",
          "type": "image",
          "description": "Medium Icon for your App, for PWA loading"
        },
        {
          "key": "icon512",
          "name": "Icon 512 x 512",
          "type": "image",
          "description": "Large Icon for your App, for PWA loading"
        }
      ]
    }
  ]
}